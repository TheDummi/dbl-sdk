{
	"name": "dbl-sdk",
	"description": "A Discord Bot List SDK to cover all supported API's.",
	"version": "2.0.0",
	"type": "module",
	"scripts": {
		"start": "npm i && npm audit fix -f && npm fund && tsc -w",
		"build": "npm i && tsc && npx prettier --write .",
		"dev": "npm i && tsc -w",
		"build:esm": "tsc",
		"build:cjs": "tsc --module CommonJS --outDir dist/cjs"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TheDummi/dbl-sdk.git"
	},
	"keywords": [
		"discord",
		"bot list",
		"discord bot list"
	],
	"author": "Dummi",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/TheDummi/dbl-sdk/issues"
	},
	"homepage": "https://github.com/TheDummi/dbl-sdk#readme",
	"devDependencies": {
		"prettier": "^3.1.0",
		"typescript": "^4.3.2",
		"xernerx": "^4.11.0"
	},
	"dependencies": {
		"zod": "^3.22.4"
	}
}
